[user]
	email = szczygiel.p@gmail.com
	name = Piotr Szczygie≈Ç
[apply]
	whitespace = warn
[core]
	autocrlf = input
	editor = vim
[url "git@github.com:"]
	insteadOf = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[push]
	default = simple
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
